<!DOCTYPE HTML>

<!-- custom header because I'll be using some dank js libraries-->
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="description" content="Redesigned Stroke Team internal website!">
        <title>Stroke Website</title>

        <!-- Bootstrap 4.1.3 CSS ONLY-->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
        <link rel="stylesheet" href="/jqueryui/jquery-ui.min.css">

        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.bundle.min.js" integrity="sha256-XF29CBwU1MWLaGEnsELogU6Y6rcc5nCkhhx89nFMIDQ=" crossorigin="anonymous"></script>
        <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
        <script src="/jqueryui/jquery-ui.min.js"></script>
        <script src="/js/functions.js"></script>
		

		<!-- Custom CSS -->
		<link rel="stylesheet" href="/stylesheets/style.css">

    </head>

    <body class="normal-page">
        <!-- <h4>This is from the header!</h4> -->

<div class="container-fluid container-offset">

	<% include ./partials/navbar %>
	<%var FirstName = "Admin"; var LastName = "Chaudhari"; var Email = "gunvantc@ucla.edu"; var TB_date = "May 1st, 2019";%>
	
	<div class="row">
		<div class="col welcome">
			Welcome, <%=FirstName%>
		</div>

		<% if (FirstName == "Admin"){%>
			<div class="col" style="padding-top: 25px;">
			<a href="/admin">Click here for admin pages</a>
		</div>
		<% }%>

	</div>

	<div class = "row row-outer">
		<div class="col-md-4 text-center">
			<div class = "row">
				<div class="col element-box">
					<h1> My info </h1>
					
					<p> Name: <%=FirstName%>&nbsp<%=LastName%></p>
					<p> Email: <%=Email%></p>					
					<p> TB Test Date Expiration: <%=TB_date%></p>
				</div>
			</div>
			<div class = "row">
				<div class="col element-box">
					<h1> Recent Time-Ins </h1>
					<table class="table table-striped table-sm">
					  	<thead class="thead-light">
					    	<tr>
						      	<th scope="col">#</th>
						      	<th scope="col">Student</th>
						      	<th scope="col">Time-in</th>
						      	<th scope="col">Duration</th>
					    	</tr>
					  	</thead>
					  	<tbody>
						    <tr>
						      	<th scope="row">1</th>
						      	<td>Mark</td>
						      	<td>Otto</td>
						      	<td>@mdo</td>
						    </tr>
						    <tr>
						      	<th scope="row">2</th>
						      	<td>Jacob</td>
						      	<td>Thornton</td>
						      	<td>@fat</td>
						    </tr>
						    <tr>
						      	<th scope="row">3</th>
						      	<td>Larry</td>
						      	<td>the Bird</td>
						      	<td>@twitter</td>
						    </tr>
					  	</tbody>
					</table>
				</div>
			</div>
		</div>
		<div class="col-md-8 text-center">
			<div class = "row">
				<div class = "col element-box">
					<h1> My Hours</h1>
					<div class="row">
						<div class="col-sm-4">
							<!-- Pass in all the data to this site -->
							<% var list_with_quarters = ["Summer","Fall","Spring","Winter"];
							var list_with_years = ["2019","2018","2017","2016"];%>

							<div class="form-group form-inline">
					        	<label style="margin-top: 10px; margin-right: 20px;" class="mx-auto" for="quarter">Quarter</label>
					        	<select class="form-control mx-auto" id="quarter">
					        		<%for (i=0; i<list_with_quarters.length; i++) {%>
					        		<option><%=list_with_quarters[i]%></option>
					        		<%}%>
					        	</select>
					        </div>

							<div class="form-group form-inline">
					        	<label style="margin-top: 10px; margin-right: 20px;" class="mx-auto" for="year">Year</label>
					        	<select class="form-control mx-auto" id="year">
					        		<%for (i=0; i<list_with_years.length; i++) {%>
					        		<option><%=list_with_years[i]%></option>
					        		<%}%>
					        	</select>
					        </div>

					        <button type="submit" action="cha" class="btn btn-primary">Submit</button>

						</div>
						<div class="col-sm-8">
							<table class="table table-striped table-sm">
							  	<thead class="thead-light">
							    	<tr>
								      	<th scope="col">Date</th>
								      	<th scope="col">Time-in</th>
								      	<th scope="col">Duration</th>
							    	</tr>
							  	</thead>
							  	<tbody id="my-hours-table">
								    <tr>
								      	<td>Mark</td>
								      	<td>Otto</td>
								      	<td>@mdo</td>
								    </tr>
								    <tr>
								      	<td>Jacob</td>
								      	<td>Thornton</td>
								      	<td>@fat</td>
								    </tr>
								    <tr>
								      	<td>Larry</td>
								      	<td>the Bird</td>
								      	<td>@twitter</td>
								    </tr>
							  	</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
			<div class = "row">
				<div class = "col element-box">
					<h1> Hour Graph All Time</h1>
					<canvas id="all-time-graph" height="50"></canvas>
				</div>
			</div>
		</div>
	</div>
	
	<div class = "row row-outer">
		<div class="col text-center element-box">
			<h1> All Sudents Hours </h1>
			<canvas id="all-student-graph" height="50"></canvas>
		</div>
	</div>

	<script>

	var ctx = document.getElementById("all-time-graph");
	var all_time_graph = new Chart(ctx, {
	    type: 'bar',
	    data: {
	        labels: ["Red", "Blue", "Yellow", "Green", "Purple", "Orange"],
	        datasets: [{
	            label: '# of Votes',
	            data: [12, 19, 3, 5, 2, 3],
	            backgroundColor: [
	                'rgba(255, 99, 132, 0.2)',
	                'rgba(54, 162, 235, 0.2)',
	                'rgba(255, 206, 86, 0.2)',
	                'rgba(75, 192, 192, 0.2)',
	                'rgba(153, 102, 255, 0.2)',
	                'rgba(255, 159, 64, 0.2)'
	            ],
	            borderColor: [
	                'rgba(255,99,132,1)',
	                'rgba(54, 162, 235, 1)',
	                'rgba(255, 206, 86, 1)',
	                'rgba(75, 192, 192, 1)',
	                'rgba(153, 102, 255, 1)',
	                'rgba(255, 159, 64, 1)'
	            ],
	            borderWidth: 1
	        }]
	    },
	    options: {
	        scales: {
	            yAxes: [{
	                ticks: {
	                    beginAtZero:true
	                }
	            }]
	        }
	    }
	});		

	var gtx = document.getElementById("all-student-graph");
	var all_student_graph = new Chart(gtx, {
	    type: 'bar',
	    data: {
	        labels: ["Red", "Blue", "Yellow", "Green", "Purple", "Orange"],
	        datasets: [{
	            label: '# of Votes',
	            data: [12, 19, 3, 5, 2, 3],
	            backgroundColor: [
	                'rgba(255, 99, 132, 0.2)',
	                'rgba(54, 162, 235, 0.2)',
	                'rgba(255, 206, 86, 0.2)',
	                'rgba(75, 192, 192, 0.2)',
	                'rgba(153, 102, 255, 0.2)',
	                'rgba(255, 159, 64, 0.2)'
	            ],
	            borderColor: [
	                'rgba(255,99,132,1)',
	                'rgba(54, 162, 235, 1)',
	                'rgba(255, 206, 86, 1)',
	                'rgba(75, 192, 192, 1)',
	                'rgba(153, 102, 255, 1)',
	                'rgba(255, 159, 64, 1)'
	            ],
	            borderWidth: 1
	        }]
	    },
	    options: {
	        scales: {
	            yAxes: [{
	                ticks: {
	                    beginAtZero:true
	                }
	            }]
	        }
	    }
	});




	</script>

<% include ./partials/footer %>
