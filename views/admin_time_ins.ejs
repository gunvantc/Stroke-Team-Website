<% include ./partials/header %>

<div class="container-fluid container-offset">

	<% include ./partials/navbar %>

<div class="row" style="min-height: calc(100vh - 89px);">
	
	<% include ./partials/admin_sidebar %>
	<div class = "col-md-10 text-center">
		<h1> Recent Time-Ins </h1>
		<br>
		<form style="padding-bottom:10px">
			<div class="btn-group">

				<button type="submit" class="btn btn-outline-light btn-scrl <%if (page==1) {%>disabled<%}%>" formmethod="post" formaction="/admin/time-ins/lscrl"> < </button>
						&nbsp&nbsp&nbsp&nbsp&nbsp
						<span> P A G E&nbsp&nbsp<%=page%> </span>
						&nbsp&nbsp&nbsp&nbsp&nbsp
				<button type="submit" class="btn btn-outline-light btn-scrl" formmethod="post" formaction="/admin/time-ins/rscrl"> > </button>
			</div>
		</form>


		<table class="table table-striped table-sm" style="overflow-x: auto;">
		  	<thead class="thead-light">
			    	<tr>
			    		<th scope="col">Name</th>
				      	<th scope="col">Time-in</th>
				      	<th scope="col">Duration</th>
		    		</tr>
		  	</thead>
		  	<tbody>
		  		<%var options = { weekday: 'short', year: 'numeric', month: 'short', day: 'numeric', hour: '2-digit', minute: '2-digit' };%>
			    <%for (var key in entries) {%>	
			    	<tr>
			      		<td><%=entries[key].member.fName%> <%=entries[key].member.lName%></td>
			      		<td><%=entries[key].start.toLocaleDateString("en-US",options)%></td>
			      		<td><%if(entries[key].dur) {%>
			      				<%=(entries[key].dur/1000/3600).toFixed(2)%> hrs
			      			<%}%>
			      		</td>
			    	</tr>
			   	<%}%>
		  	</tbody>
		</table>
	</div>

</div>


<% include ./partials/footer %>